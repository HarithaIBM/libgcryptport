export ZOPEN_TARBALL_URL="https://gnupg.org/ftp/gcrypt/libgcrypt/libgcrypt-1.10.2.tar.bz2"
export ZOPEN_TARBALL_DEPS="make bzip2 tar curl perl automake autoconf m4 sed gettext zoslib diffutils libgpgerror"
export ZOPEN_TYPE="TARBALL"
export ZOPEN_EXTRA_CFLAGS="${ZOPEN_EXTRA_CFLAGS} -O3"
export ZOPEN_COMP=CLANG
LIBGCRYPT_VERSION="1.10.2"

zopen_check_results()
{
  dir="$1"
  pfx="$2"
  chk="$1/$2_check.log"
  
 failures=$(egrep "^FAIL:" $chk | wc -l)
  skipped=$(egrep "^SKIP:" $chk | wc -l)
  passed=$(egrep "^PASS:" $chk | wc -l)
  totalTests=$(($failures+$skipped+$passed))

  # Echo the following information to gauge build health
cat <<ZZ
actualFailures:${failures}
actualSkipped:${skipped}
totalTests:${totalTests}
expectedFailures:0
ZZ
}

zopen_append_to_env()
{
  # echo envars outside of PATH, MANPATH, LIBPATH
}

zopen_append_to_setup()
{
  # echo commands that will run when installing via setup.sh
}

zopen_get_version()
{
  # Modify to echo the version of your tool/library
  # Rather than hardcoding the version, obtain the version by running the tool/library
  echo "$LIBGCRYPT_VERSION"
}
